	<h1>Sugerencias</h1>

	{{#each sugerencias}}

		{{#each this.atuendo}}
			<h3> Atuendo </h3>
			<ul>
				<li>Color primario: {{this.colorPrimario}}</li>
				<li>Color secundario: {{this.colorSecundario}}</li>
				<li>Tipo: {{this.tipo}}</li>
				<li>Tela: {{this.tela}}</li>
			</ul>

		{{/each}}
		<h3>Estado: </h3> {{this.estado}}
	{{/each}}
